		<servlet-name>GraficaServlet</servlet-name>
		<servlet-class>es.isst.glucomed.GraficaServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>GraficaServlet</servlet-name>
		<url-pattern>/grafica</url-pattern>
